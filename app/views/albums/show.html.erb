<section style="padding-left:20px;">
<p id="notice"><%= notice %></p>

<h1><%= @album.album_title %></h1>

<%= image_tag @album.album_artwork.url(:medium) %>


<ul>
<% @tracks.each do |song| %>
  <li><%= link_to "#{song.title}", master_song_path(song) %></li>
<% end %>
</ul>

<%= link_to 'Edit', edit_album_path(@album) %> |
<%= link_to 'Back', :back, remote: :true, :id => "page-link" %> |
<%= link_to 'Add Song', add_album_song_master_songs_path, remote: :true, :id => "page-link" %>
</section>